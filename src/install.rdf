<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
        <em:id>unmark@neurotechnics.com</em:id>
        <em:type>2</em:type>
        <em:name>Unmark</em:name>
        <em:version>{%PACKAGE_VERSION%}</em:version>
        <em:description>Quickly add the current page to your Unmark.it bookmark to-do list</em:description>
        <em:creator>Neurotechnics</em:creator>
        <em:developer>James Price</em:developer>
        <em:contributor></em:contributor>
        <em:translator></em:translator>
        <em:homepageURL>http://www.neurotechnics.com/blog/unmark-firefox-extension/</em:homepageURL>
        <em:iconURL>chrome://unmark/skin/logo.png</em:iconURL>
        <em:optionsURL>chrome://unmark/content/options.xul</em:optionsURL>

        <!-- Firefox -->
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>21.0</em:minVersion>
                <em:maxVersion>31.0</em:maxVersion>
            </Description>
        </em:targetApplication>
        <!-- /Firefox -->
    </Description>


</RDF>
